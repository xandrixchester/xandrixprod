<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Xandrixprod</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="title-banner">
    <div class="title-text col-10 offset-1 text-center">
      <h1>Xandrix Production</h1>
    </div>  
  </div>
  
  <div class="booyah">
    <div class="intro-banner">
        <div class="intro col-10 offset-1 text-center ">
            <h3>A FREELANCE <br>PHOTOGRAPHER <br> and <br>WEB DEVELOPER</h3>
        </div>
    </div>
  </div>
   
<div class="booyah-main-photo col-10 offset-1">
  <div class="photo-port">
      <div class="photo_text col-10 offset-1 text-center">
          <h4>Photography Portfolio</h4>
      </div>
      <div class="booyah-sm col-10 offset-1 text-center">
        <div class="row feature text-center">
          <div class="col-12 col-sm">
            <div class="col-12 col-sm-12">
                Example 1
                <%= image_tag 'steph1.jpg', class: 'img-fluid' %>
            </div>
          </div>

          <div class="col-12 col-sm">
            <div class="col-12 col-sm-12">
                Example 2
                <%= image_tag 'steph2.jpg', class: 'img-fluid' %>
            </div>

          </div>

          <div class="col-12 col-sm">
            <div class="col-12 col-sm-12">
                Example 3
                <%= image_tag 'steph3.jpg', class: 'img-fluid' %>
            </div>
          </div>
        </div>
      </div>
  </div>
</div>


<div class="booyah-main-web col-10 offset-1">
  <div class="web-port">
    <div class="web_text col-10 offset-1 text-center">
    <h4>Web-Development Portfolio</h4>
    </div>

      <div class="booyah-sm col-10 offset-1 text-center">

          <div class="row feature text-center">

            <div class="col-12 col-sm">
              <div class="col-12 col-sm-12">
              Splurty Place_holder
              <%= image_tag 'steph3.jpg', class: 'img-fluid' %>
              </div>
            </div>

            <div class="col-12 col-sm">
              <div class="col-12 col-sm-12">
              Nomster Place_holder
              <%= image_tag 'steph3.jpg', class: 'img-fluid' %>
              </div>
            </div>

            <div class="col-12 col-sm">
              <div class="col-12 col-sm-12">
              Flixter Place_holder
              <%= image_tag 'steph3.jpg', class: 'img-fluid' %>
              </div>
            </div>
        </div>
    </div>
  </div>
</div>




    <%= yield %>
  </body>
</html>
