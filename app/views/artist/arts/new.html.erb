<br />
<div class="booyah-box col-10 offset-1">
  <h1>Submit your art</h1>

  <%= simple_form_for @art, url: artist_arts_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />


</div>